#include<iostream>
#include<cstring> 
#include<cstdio>
using namespace std;
char a[1000005],b[1000005];
int F[1000005];
int la,lb;
void get_next(){
	int i = 0;
	int j;
	j = F[0] = -1; 
	while(i < lb){
		while(j != -1 && b[j] != b[i]){
			j = F[j];
		}
		F[++i] = ++j;
	}
}
int kmp(){
	int i = 0,j = 0,sum = 0;
	get_next();
	while(i < la){
		while(a[i] != b[j] && j != -1){
			j = F[j];
		}
		++i;
		++j;
		if(j == lb){
			printf("%d\n",i - j + 1);
			j = F[j];
		}
	}
	return 0;
}
int main(){
	scanf("%s",a);
	scanf("%s",b);
	la = strlen(a);
	lb = strlen(b);
	kmp();
	
	for(int i = 1; i <= lb; i++){
		if(i != 1) printf(" ");
		printf("%d",F[i]);
	}
	return 0;
}
