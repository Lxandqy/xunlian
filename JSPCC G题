#include<iostream>
#include<cmath>

using namespace std;

const int mod = 1e9 + 7;

typedef long long ll;

ll a[5][5],b[5][5],c[5][5];

ll qju_a(){
	for(int i = 1; i <= 2; i++){
		for(int j = 1; j <= 2; j++){
			c[i][j] = a[i][j],a[i][j] = 0;
		}
	}
	
	
	for(int i = 1; i <= 2; i++){
		for(int j = 1; j <= 2; j++){
			int sum = 0;
			for(int k = 1; k <= 2; k++){
				sum = (sum + c[i][k] * c[k][j] % mod) % mod;
			}
			a[i][j] = sum;
		}
	}	
}

ll qju_b(){
	for(int i = 1; i <= 2; i++){
		for(int j = 1; j <= 1; j++){
			c[i][j] = b[i][j],b[i][j] = 0;
		}
	}
	
	for(int i = 1; i <= 2; i++){
		for(int j = 1; j <= 1; j++){
			int sum = 0;
			for(int k = 1; k <= 2; k++){
				sum = (sum + a[i][k] * c[k][j] % mod) % mod;
			}
			b[i][j] = sum;
		}
	}	
}

ll qpow(ll b){
	ll s = 1;
	while(b){
		if(b & 1){
			qju_b();
		}
		qju_a();
		b >>= 1;
	}
	return s;
}

ll qm(ll a,ll b){
	ll s = 1;
	while(b){
		if(b & 1){
			s = s * a % mod;
		}
		b >>= 1;
		a = a * a % mod;
	}
	return s;
}

int main(){
	int t;
	cin >> t;
	while(t--){
		int x,y;
		cin >> x >> y;
		
		if(x == 1 && y == 1){
			cout << 2 << endl;
			continue;
		}
		
		if(x == 1){
			b[1][1] = 4;
			b[2][1] = 2;
			a[1][1] = 1;
			a[1][2] = 1;
			a[2][1] = 1;
			a[2][2] = 0;
			qpow(y - 2);
			cout << b[1][1] % mod << endl;
			continue;
		}
		
		if(y == 1){
			b[1][1] = 4;
			b[2][1] = 2;
			a[1][1] = 1;
			a[1][2] = 1;
			a[2][1] = 1;
			a[2][2] = 0;
			qpow(x - 2);
			cout << b[1][1] % mod << endl;
			continue;
		}
		
		if(x == 2){
			b[1][1] = 4;
			b[2][1] = 2;
			a[1][1] = 1;
			a[1][2] = 1;
			a[2][1] = 1;
			a[2][2] = 0;
			qpow(y - 2);
			cout << 1ll * b[1][1] * b[1][1] % mod << endl;
			continue;
		}
		
		if(y == 2){
			b[1][1] = 4;
			b[2][1] = 2;
			a[1][1] = 1;
			a[1][2] = 1;
			a[2][1] = 1;
			a[2][2] = 0;
			qpow(x - 2);
			cout << 1ll * b[1][1] * b[1][1] % mod << endl;
			continue;
		}
		
		if(x == 3 && y == 4){
			cout << 44 << endl;
			continue;
		}
		
		if(x == 4 && y == 3){
			cout << 44 << endl;
			continue;
		}
		if(x == 5 && y == 3){
			cout << 64 << endl;
			continue;
		}
		
		if(y == 3 && x == 5){
			cout << 64 << endl;
			continue;
		}
		
		if(x == 3 && y > 4){
			b[1][1] = 60;
			b[2][1] = 40;
			a[1][1] = 1;
			a[1][2] = 1;
			a[2][1] = 1;
			a[2][2] = 0;
			qpow(y - 5);
			ll h = b[1][1];
			cout <<  (h + 4) % mod << endl;
			continue;
		}
		
		if(y == 3 && x > 4){
			b[1][1] = 60;
			b[2][1] = 40;
			a[1][1] = 1;
			a[1][2] = 1;
			a[2][1] = 1;
			a[2][2] = 0;
			qpow(x - 5);
			ll h = b[1][1];
			cout << (h + 4) % mod << endl;
			continue;
		}
			
		if(x == 3 && y == 3){
			cout << "32" << endl;
			continue;
		}
		
		if(x == 4 && y == 4){
			cout << "18" << endl;
			continue;
		}
		
		if(x == 4 && y > 4){
			cout << "14" << endl;
			continue;
		}
		
		if(y == 4 && x > 4){
			cout << "14" << endl;
			continue;
		}
		
		if(x >= 5 && y >= 5){
			cout << "8" << endl;
			continue;
		}
		
	}

	return 0;
}
