//理解了题意就真的很简单了。。
//比如1 0 0， 1 0 0，1 0 0 和 1 0 0，0 0 1，0 1 0；
//首个|的值就是4 第二个的值就是7 但是他们的值分别是4 4 4 和 4 2 1所以不是最小的一定是最好的；
#include <bits/stdc++.h>
using namespace std;
const int N=205;
int n,m,a[N],b[N];
signed main(){
	scanf("%d%d",&n,&m);
	for(int i=1;i<=n;i++) scanf("%d",&a[i]);
	for(int i=1;i<=m;i++) scanf("%d",&b[i]);
	for(int i=0;i<=1000;i++){
		bool flag=true;
		for(int j=1;j<=n;j++){
			bool orz=false;
			for(int k=1;k<=m;k++)
				if((a[j]&b[k]|i)==i) {orz=true;break;}
			if(!orz) {flag=false;break;}
		}
		if(flag) {printf("%d\n",i);return 0;}
	}
}
